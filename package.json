{
  "name": "dtext-vscode",
  "displayName": "DText (e621)",
  "description": "Syntax highlighting for e621's DText.",
  "version": "0.0.1",
  "publisher": "JustinMorris",
  "license": "GPL-3.0-only",
  "author": {
	"name": "Justin Morris"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "repository": {
	"url": "https://github.com/jmortiger/dtext-vscode"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "dtext",
      "aliases": ["DText (e621)", "dtext"],
      "extensions": [".dtext"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dtext",
      "scopeName": "source.dtext",
      "path": "./syntaxes/dtext.tmLanguage.json"
    }]
  }
}
